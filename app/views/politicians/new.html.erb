<h2>Add a Politican</h2>
  <%= form_for @politician do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :party %>
    <%= f.text_field :party %>
    <%= f.label :current_office %>
    <%= f.text_field :current_office %>
    <%= f.label :running_for %>
    <%= f.text_field :running_for %>
    <%= f.label :photo_url %>
    <%= f.text_field :running_for %>
    <%= f.label :elect_date %>
    <%= f.date_select :elect_date %>
    <%= f.submit "Add Politician" %>
  <% end %>
